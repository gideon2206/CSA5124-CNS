#include <stdio.h>
#include <math.h>
int power(int a, int b, int q) {
    int result = 1;
    for(int i = 0; i < b; i++)
        result = (result * a) % q;
    return result;
}

int main() {
    int q = 353;      
    int alpha = 3;  

    int XA = 97;    
    int XB = 223;     

    int YA, YB;      
    int KA, KB;       

    YA = power(alpha, XA, q); 
    YB = power(alpha, XB, q);   

    KA = power(YB, XA, q);     
    KB = power(YA, XB, q);      

    printf("Gideon Public Key (YA): %d\n", YA);
    printf("mukil Public Key (YB): %d\n", YB);
    printf("Shared Key at Gideon: %d\n", KA);
    printf("Shared Key at mukil  : %d\n", KB);

    return 0;
}

output
Gideon Public Key (YA): 40
mukil Public Key (YB): 125
Shared Key at Gideon: 77
Shared Key at mukil  : 77
